{
  "version": 2,
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "env": {
    "VITE_SUPABASE_URL": "@vite_supabase_url",
    "VITE_SUPABASE_ANON_KEY": "@vite_supabase_anon_key",
    "VITE_SUPABASE_FUNCTIONS_URL": "@vite_supabase_functions_url",
    "VITE_FAL_API_KEY": "@vite_fal_api_key",
    "VITE_FAL_API_BASE_URL": "@vite_fal_api_base_url",
    "VITE_ELEVENLABS_API_BASE_URL": "@vite_elevenlabs_api_base_url",
    "VITE_OPENROUTER_API_BASE_URL": "@vite_openrouter_api_base_url",
    "VITE_APP_NAME": "@vite_app_name",
    "VITE_APP_DOMAIN": "@vite_app_domain"
  }
} 